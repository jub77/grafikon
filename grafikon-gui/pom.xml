<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>net.parostroj</groupId>
        <artifactId>grafikon</artifactId>
        <version>1</version>
    </parent>
    <artifactId>grafikon-gui</artifactId>
    <version>${grafikon.version}</version>
    <packaging>jar</packaging>
    <name>grafikon-gui</name>
    <build>
        <resources>
            <resource>
                <directory>src/main/resources</directory>
            </resource>
            <resource>
                <directory>src/main/settings</directory>
                <filtering>true</filtering>
            </resource>
        </resources>
    </build>
    <dependencies>
        <dependency>
            <groupId>net.parostroj</groupId>
            <artifactId>grafikon-model</artifactId>
            <version>${grafikon.version}</version>
        </dependency>
        <dependency>
            <groupId>net.parostroj</groupId>
            <artifactId>grafikon-output2</artifactId>
            <version>${grafikon.version}</version>
        </dependency>
        <dependency>
            <groupId>net.parostroj</groupId>
            <artifactId>grafikon-templates</artifactId>
            <version>${grafikon.version}</version>
        </dependency>
        <dependency>
            <groupId>net.parostroj</groupId>
            <artifactId>grafikon-gui-components</artifactId>
            <version>${grafikon.version}</version>
        </dependency>
        <dependency>
            <groupId>net.parostroj</groupId>
            <artifactId>grafikon-net</artifactId>
            <version>${grafikon.version}</version>
        </dependency>
        <dependency>
            <groupId>jgraph</groupId>
            <artifactId>jgraph</artifactId>
            <version>5.13.0.0</version>
        </dependency>
        <dependency>
            <groupId>${project.groupId}</groupId>
            <artifactId>grafikon-save</artifactId>
            <version>${grafikon.version}</version>
            <scope>runtime</scope>
        </dependency>
        <dependency>
            <groupId>${project.groupId}</groupId>
            <artifactId>grafikon-ls</artifactId>
            <version>${grafikon.version}</version>
            <scope>runtime</scope>
        </dependency>
        <dependency>
            <groupId>${project.groupId}</groupId>
            <artifactId>grafikon-ls4</artifactId>
            <version>${grafikon.version}</version>
            <scope>runtime</scope>
        </dependency>
    </dependencies>
</project>
